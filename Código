#include <stdio.h>
#include <stdlib.h>

int main()
{

    int idade;
    char nome [45];
    float altura;
    char genero;

    printf("Nome:\n");
    printf("Idade:\n");
    printf("Genero:\n");
    printf("Altura:\n");

    printf("Digite sua Idade \n");
    scanf("%d", &idade);

    printf("Digite seu Nome \n");
    scanf("%s", &nome);

    printf("Digite sua Altura \n");
    scanf("%f", &altura);

     printf("Digite seu Genero \n");
    scanf("%s", &genero);

    printf("Sua idade: %d \n", idade);
    printf("Seu nome: %s \n", nome);
    printf("Sua altura: %f \n", altura);
    printf("Seu genero: %c \n", genero);

    return 0;
}

